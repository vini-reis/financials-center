{
  "compileOnSave": true,
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "outDir": "wwwroot/js",
    "allowJs": true,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "scripts/**/*"
  ],
  "exclude": [
    "wwwroot",
    "node_modules"
  ]
}
